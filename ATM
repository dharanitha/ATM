    Private Sub add()
        Try


            cmd = New SqlCommand("Update reg set otppin=" & s3 & " where account_no='" & TextBox1.Text & "'", con)
            con.Open()
            cmd.ExecuteNonQuery()
            con.Close()
        Catch ex As Exception
            MsgBox("account number does not exist")

        End Try
    End Sub

