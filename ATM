    Dim s3 As String
    Private Sub otp()

        Dim numbers As String = "112340"

        Dim characters As String = numbers

        characters += Convert.ToString(numbers)

        Dim length As Integer = 4
        Dim otp As String = String.Empty
        For i As Integer = 0 To length - 1
            Dim character As String = String.Empty
            Do
                Dim index As Integer = New Random().Next(0, characters.Length)
                character = characters.ToCharArray()(index).ToString()
            Loop While otp.IndexOf(character) <> -1
            otp += character
        Next
        c = otp
        'MessageBox.Show("OTP : " + c)
        'c = "3104"
        'sendSMS()
        'Dim sms As New sendSMS
        'Dim response As String
        'response = sms.sendSMS("9994472993", otp)
        Dim cmd = New SqlCommand("select pin from reg where account_no='" & TextBox1.Text & "'", con)
        Dim da = New SqlDataAdapter(cmd)
        Dim ds = New DataSet()
        da.Fill(ds, "reg")
        If ds.Tables(0).Rows.Count > 0 Then
            con.Close()
        End If

        s3 = ds.Tables(0).Rows(0)(0).ToString()

        ' pin1+otp2+pin3+otp4

        Mid(s3, 2, 1) = c.Substring(1, 1)
        Mid(s3, 4, 1) = c.Substring(3, 1)
        'Mid(c, 2, 1) = s3.Substring(1, 1)
        'Mid(c, 4, 1) = s3.Substring(3, 1)


    End Sub

