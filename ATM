    Public Sub getpin()
        Dim r As New Random
        Dim listNumbers As List(Of Integer) = New List(Of Integer)()
        Dim numbers As Integer = r.[Next](1, 9)

        For i As Integer = 0 To 4 - 1
            listNumbers.Add(numbers)
            numbers = r.[Next](1, 9)
            Label21.Text = Label21.Text + numbers.ToString()
        Next
        ' Label21.Text = numbers
        MessageBox.Show("Pin No:" + Label21.Text)
        Dim s3 As String = Label21.Text
        '  s3 = "3425"
        's3=3425
        's2=1111
        If s2 <> "" Then
            '  s2 = s2.Replace(s2.Substring(1, 2), s3.Substring(0, 2))
            ' s2 = s2.Replace(s2.Substring(3, 4), s3.Substring(0, 4))
            ' s2 = s2.Remove(1).Insert(1, s3.Substring(1, 2))
            ' s2 = s2.ReplaceAt(1, "1")
            s2 = s2.Replace(CChar("l"), CChar("d"))
            Mid(s2, 2, 1) = s3.Substring(1, 1)
            Mid(s2, 4, 1) = s3.Substring(3, 1)

        End If
        Label21.Text = s2
    End Sub
