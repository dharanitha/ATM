    Sub TypeRand()
        Dim rng As New System.Random()
        Dim RAND(16) As String

        RAND(0) = "APPLE"
        RAND(1) = "ORANGE"
        RAND(2) = "BANANA"
        RAND(3) = "GRAPE"
        RAND(4) = "FLY"
        RAND(5) = "TIGER"
        RAND(6) = "CAR"
        RAND(7) = "GENTER"
        RAND(8) = "OZONE"
        RAND(9) = "DONKEY"
        RAND(10) = "LIME"
        RAND(11) = "PLANT"
        RAND(12) = "RANGE"
        RAND(13) = "MOTHER"
        RAND(14) = "SISTER"
        RAND(15) = "SENIOR"


        Label20.Text = RAND(rng.Next(RAND.Count()))
    End Sub
