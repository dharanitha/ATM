    Private Sub ok()


        str1 = Label20.Text
        s = str1.Length()

        If s = "4" Then
            s1 = Val(TextBox2.Text) - 4444

        ElseIf s = "5" Then
            s1 = Val(TextBox2.Text) - 5555

        ElseIf s = "6" Then
            s1 = Val(TextBox2.Text) - 6666

        ElseIf s = "7" Then
            s1 = Val(TextBox2.Text) - 7777

        ElseIf s = "1" Then
            s1 = Val(TextBox2.Text) - 1111

        ElseIf s = "2" Then
            s1 = Val(TextBox2.Text) - 2222

        ElseIf s = "3" Then
            s1 = Val(TextBox2.Text) - 3333

        ElseIf s = "8" Then
            s1 = Val(TextBox2.Text) - 8888

        ElseIf s = "9" Then
            s1 = Val(TextBox2.Text) - 9999

        End If
    End Sub

